tmpfs			/dev/shm		tmpfs	mode=0777	0	0
overlay			/etc			overlay	lowerdir=/media/sdcard/root/etc,upperdir=/media/sdcard/data/overlay/etc,workdir=/media/sdcard/data/overlay/.workdir/etc	0	0
